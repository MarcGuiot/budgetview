grant principal org.apache.wicket.security.hive.authorization.SimplePrincipal "basic"
{
    permission ${ComponentPermission} "org.crossbowlabs.rooms.web.pages.Index", "inherit, render";
    permission ${ComponentPermission} "org.crossbowlabs.rooms.web.pages.Index", "enable";
    permission ${ComponentPermission} "org.crossbowlabs.rooms.web.pages.RoomPage", "inherit, render";
    permission ${ComponentPermission} "org.crossbowlabs.rooms.web.pages.RoomPage", "enable";
    permission ${ComponentPermission} "org.crossbowlabs.rooms.web.pages.RoomsPage", "inherit, render";
    permission ${ComponentPermission} "org.crossbowlabs.rooms.web.pages.RoomsPage", "enable";
};

