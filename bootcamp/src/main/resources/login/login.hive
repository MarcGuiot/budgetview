    permission ${ComponentPermission} "org.crossbowlabs.rooms.web.pages.RoomsAdminPage", "inherit, render";
    permission ${ComponentPermission} "org.crossbowlabs.rooms.web.pages.RoomsAdminPage", "enable";
    permission ${ComponentPermission} "org.crossbowlabs.rooms.web.pages.BarcoAdminPage", "inherit, render";
    permission ${ComponentPermission} "org.crossbowlabs.rooms.web.pages.BarcoAdminPage", "enable";
