<?xml version="1.0" encoding="UTF-8" ?>
<globs>
  <bank name="La Poste" id='3'>
    <bankEntity id="20041"/>

    <transactionTypeMatcher regexp="ACHAT CB (.*)[\s]+(\d{2,4}.\d{2}.\d{2,4})"
                            transactionTypeName="credit_card" label="$1" groupForDate="2" dateFormat="dd.MM.yy"/>

    <transactionTypeMatcher regexp="REMISE DE CHEQUES DU (\d{2,4}/\d{2}/\d{2,4})"
                            transactionTypeName="deposit" label="REMISE CHEQUES" groupForDate="1"
                            dateFormat="dd/MM/yy"/>

    <transactionTypeMatcher regexp="CHEQUE N. (.*)"
                            transactionTypeName="check" label="CHEQUE N. $1"/>

    <transactionTypeMatcher regexp="CARTE ([\w]+) (\d{2,4}/\d{2}/\d{2,4}) A ([0-9H]+)"
                            transactionTypeName="withdrawal" label="RETRAIT $1 $3" groupForDate="2"
                            dateFormat="dd/MM/yy"/>

  </bank>
</globs>