<?xml version="1.0" encoding="UTF-8" ?>
<globs>
  <bank name="CIC" downloadUrl="http://www.cic.fr/telechargements.cgi" id='2'>
    <bankEntity id="30066"/>
    <bankEntity id="10037"/>
    <bankEntity id="10057"/>
    <bankEntity id="10096"/>
    <bankEntity id="12319"/>
    <bankEntity id="12719"/>
    <bankEntity id="30027"/>
    <bankEntity id="30047"/>
    <bankEntity id="30087"/>
    <bankEntity id="30568"/>

    <transactionTypeMatcher regexp="F COM INTERVENTION (.*)"
                            transactionTypeName="bank_fees" label="COMMISSIONS $1"/>

    <transactionTypeMatcher regexp="ABON FBQ (.*)"
                            transactionTypeName="bank_fees" label="FILBANQUE $1"/>

    <transactionTypeMatcher regexp="RETRAIT DAB [0-9]* (.*)"
                            transactionTypeName="withdrawal" label="RETRAIT $1"/>

    <transactionTypeMatcher regexp="RETRAIT (.*)"
                            transactionTypeName="withdrawal" label="RETRAIT $1"/>

    <transactionTypeMatcher regexp="ECH PRET (.*)"
                            transactionTypeName="credit" label="PRET $1"/>

    <transactionTypeMatcher regexp="VIR (.*)"
                            transactionTypeName="virement" label="$1"/>

    <transactionTypeMatcher regexp="REM CHQ (.*)"
                            transactionTypeName="deposit" label="REMISE CHEQUES $1"/>

    <transactionTypeMatcher regexp="CHEQUE[ ]+(.*)"
                            transactionTypeName="check" label="CHEQUE N. $1"/>
  </bank>
</globs>