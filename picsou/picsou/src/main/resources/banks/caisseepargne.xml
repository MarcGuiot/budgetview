    <transactionMatcher qifP="ECH PRET" transactionTypeName="credit"
                        label="{P.0}" originalLabel="{P.0}"/>

    <transactionMatcher qifP="RETRAIT DAB (.*)" transactionTypeName="withdrawal"
                        label="RETRAIT {P.1}" originalLabel="{P.0}"/>

