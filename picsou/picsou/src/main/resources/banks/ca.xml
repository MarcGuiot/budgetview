<globs>
  <bank name="Credit Agricole" id='1'>
    <bankEntity id="13006"/>

    <transactionTypeMatcher regexp="REGLEM ECHEANCE (.*)"
                            transactionTypeName="credit" label="PRET $1"/>

    <transactionTypeMatcher regexp="RETRAIT(.*)"
                            transactionTypeName="withdrawal" label="RETRAIT $1"/>

    <transactionTypeMatcher regexp="INTERETS DEBITEURS"
                            transactionTypeName="bank_fees" label="$0"/>

    <transactionTypeMatcher regexp="REMISE(.*)"
                            transactionTypeName="deposit" label="REMISE CHEQUES $1"/>

    <transactionTypeMatcher regexp="VIR. PERMANENT"
                            transactionTypeName="virement" label="VIR. $1"/>

    <transactionTypeMatcher regexp="CHEQUE[ ]+(.*)"
                            transactionTypeName="check"  label="CHEQUE N. $1"/>
  </bank>
</globs>
