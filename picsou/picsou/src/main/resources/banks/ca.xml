<globs>
  <bank name="creadit agricole">
    <bankEntity id="13006"/>

    <transactionTypeMatcher regexp="REGLEM ECHEANCE (.*)"
                            transactionTypeName="credit" groupForLabel="1"/>

    <transactionTypeMatcher regexp="RETRAIT(.*)"
                            transactionTypeName="withdrawal" groupForLabel="1"/>

    <transactionTypeMatcher regexp="INTERETS DEBITEURS"
                            transactionTypeName="bank_fees"/>

    <transactionTypeMatcher regexp="REMISE(.*)"
                            transactionTypeName="deposit" groupForLabel="1"/>

    <transactionTypeMatcher regexp="VIR. PERMANENT"
                            transactionTypeName="virement"/>

    <transactionTypeMatcher regexp="CHEQUE[ ]+(.*)"
                            transactionTypeName="check" groupForLabel="1"/>
  </bank>
</globs>
