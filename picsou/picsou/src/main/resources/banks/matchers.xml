<globs>
  <!-- Warning: the ordering of these matchers is important -->

  <!-- CHECKS -->

  <transactionTypeMatcher regexp="CHEQUE N. (.*)"
                          transactionTypeName="check" groupForLabel="1"/>

  <transactionTypeMatcher regexp="CHEQUE[ ]+(.*)"
                          transactionTypeName="check" groupForLabel="1"/>

  <!-- WITHDRAWALS -->

  <transactionTypeMatcher regexp="RET\. DAB[ ]+[\w]+[ ]+([0-9/]+)[ ]+(.*)"
                          transactionTypeName="withdrawal" groupForLabel="2" groupForDate="1"/>

  <transactionTypeMatcher regexp="RETRAIT DAB [0-9]* (.*)"
                          transactionTypeName="withdrawal" groupForLabel="1"/>

  <transactionTypeMatcher regexp="CARTE[ ]+[0-9]+[ ]+RETRAIT DAB[ ]*[\w]*[ ]+([0-9/]+)[ ]+(.*)"
                          transactionTypeName="withdrawal" groupForLabel="2" groupForDate="1"/>

  <transactionTypeMatcher regexp="RET ECLAIR[ ]+[\w]+[ ]+([0-9/]+)[ ]+(.*)"
                          transactionTypeName="withdrawal" groupForLabel="2" groupForDate="1"/>

  <transactionTypeMatcher regexp="RETRAIT (.*)"
                          transactionTypeName="withdrawal" groupForLabel="1"/>

  <!---->

  <transactionTypeMatcher regexp="PRLV (.*)"
                          transactionTypeName="prelevement" groupForLabel="1"/>

  <transactionTypeMatcher regexp="REGLEM ECHEANCE (.*)"
                          transactionTypeName="credit" groupForLabel="1"/>

  <transactionTypeMatcher regexp="PRLV (.*)"
                          transactionTypeName="prelevement" groupForLabel="1"/>

  <transactionTypeMatcher regexp="ECH PRET (.*)"
                          transactionTypeName="credit" groupForLabel="1"/>

  <transactionTypeMatcher regexp="VIR (.*)"
                          transactionTypeName="virement" groupForLabel="1"/>

  <transactionTypeMatcher regexp="REM CHQ (.*)"
                          transactionTypeName="deposit"/>

  <transactionTypeMatcher regexp="ACHAT CB (.*)"
                          transactionTypeName="credit_card" groupForLabel="1"/>

  <transactionTypeMatcher regexp="REMISE DE CHEQUES DU (.*)"
                          transactionTypeName="deposit"/>

  <transactionTypeMatcher regexp="FAC\.FRANCE[ ]+[\w]+[ ]+([0-9/]+)[ ]+(.*)"
                          transactionTypeName="credit_card" groupForLabel="2" groupForDate="1"/>

  <transactionTypeMatcher regexp="CARTE[ ]+[0-9]+[ ]+([0-9/]+)[ ]+(.*)"
                          transactionTypeName="credit_card" groupForLabel="2" groupForDate="1"/>

  <transactionTypeMatcher regexp="PRELEVEMENT[ ]+[\w]+[ ]+(.*)"
                          transactionTypeName="prelevement" groupForLabel="1"/>

  <transactionTypeMatcher regexp="VIR.LOGITEL[ ]+[^ ]+(.*)"
                          transactionTypeName="virement" groupForLabel="1"/>

  <transactionTypeMatcher regexp="VIR.LOGITEL[ ]*"
                          transactionTypeName="virement"/>

  <transactionTypeMatcher regexp="VIREMENT[ ]+[\w]+[ ]+(.*)"
                          transactionTypeName="virement" groupForLabel="1"/>

  <!-- BANK FEES -->

  <transactionTypeMatcher regexp="INTERETS DEBITEURS"
                          transactionTypeName="bank_fees"/>

  <transactionTypeMatcher regexp="F COM INTERVENTION (.*)"
                          transactionTypeName="bank_fees"/>

  <transactionTypeMatcher regexp="ABON FBQ (.*)"
                          transactionTypeName="bank_fees"/>

</globs>