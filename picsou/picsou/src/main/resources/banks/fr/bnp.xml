<?xml version="1.0" encoding="UTF-8" ?>
<globs>
  <bankFormat id='5'>

    <bank name="BNP Paribas" url="http://www.bnpparibas.net" id='5' synchroEnabled="true" icon="banks/bnp.png">
      <bankEntity id="30004"/>

    </bank>

    <transactionMatcher bankType="COMMISS" qifM="(.*)"
                        transactionTypeName="bank_fees" label="COMMISSIONS {M.1}"
                        originalLabel="{M.0}"/>

    <transactionMatcher qifM="COMMISSIONS PERCUES[\s]*(.*)"
                        transactionTypeName="bank_fees" label="COMMISSIONS {M.1}"
                        originalLabel="{M.0}"/>

    <transactionMatcher bankType="CHQ N.*" qifM="N. (.*)"
                        transactionTypeName="check" label="CHEQUE N°{M.1}"
                        originalLabel="CHEQUE N.{M.1}"/>

    <transactionMatcher qifM="CHEQUE[\s]+N.[\s]+(.*)"
                        transactionTypeName="check" label="CHEQUE N°{M.1}"
                        originalLabel="CHEQUE N.{M.1}"/>

    <transactionMatcher bankType="ECH.PR" qifM="(.*)"
                        transactionTypeName="credit" label="PRET {M.1}"/>

    <transactionMatcher qifM="ECHEANCE PRET[\s]*(.*)"
                        transactionTypeName="credit" label="PRET {M.1}"/>

    <transactionMatcher qifM="RETRAIT DAB[\s]*(\d{2,4}/\d{2}/\d{2,4})[\s]*(.*)"
                        transactionTypeName="withdrawal" label="RETRAIT {M.2}"
                        groupForDate="{M.1}" dateFormat="dd/MM/yy"/>

    <transactionMatcher qifM="RETRAIT DAB[\s]*(.*)"
                        transactionTypeName="withdrawal" originalLabel="RETRAIT {M.1}"/>

    <transactionMatcher bankType="RETRAIT" qifM="(\d{2,4}/\d{2}/\d{2,4})[\s]* (.*)"
                        transactionTypeName="withdrawal" label="RETRAIT {M.2}"
                        groupForDate="{M.1}" dateFormat="dd/MM/yy"
                        originalLabel="{M.0}"/>

    <transactionMatcher bankType="PRLV." qifM="(.*)"
                        transactionTypeName="prelevement" label="{M.1}"
                        originalLabel="{M.1}"/>

    <transactionMatcher qifM="PRELEVEMENT[\s]*(.*)"
                        transactionTypeName="prelevement" label="{M.1}"
                        originalLabel="{M.1}"/>

    <transactionMatcher qifM="FACTURE CARTE[\s]+DU[\s]+(\d{6})[\s]+(.*)"
                        transactionTypeName="credit_card" label="{M.2}"
                        groupForDate="{M.1}" dateFormat="ddMMyy"/>

    <transactionMatcher qifM="FACTURE CARTE[\s]*(.*)"
                        transactionTypeName="credit_card" label="{M.1}"/>

    <transactionMatcher qifM="FACTURE CARTE[\s]+(.*)"
                        transactionTypeName="credit_card" originalLabel="{M.1}"/>

    <transactionMatcher qifM="REGLEMENT TIP[\s]*(.*)"
                        transactionTypeName="prelevement" label="{M.1}"
                        originalLabel="{M.1}"/>

    <transactionMatcher qifM="VIREMENT RECU TIERS[\s]*(.*)"
                        transactionTypeName="virement" label="VIREMENT DE {M.1}"
                        originalLabel="VI.TIER {M.1}"/>

    <transactionMatcher bankType="VI.TIER" qifM="(.*)"
                        transactionTypeName="virement" label="VIREMENT DE {M.0}"
                        originalLabel="VI.TIER {M.0}"/>

    <transactionMatcher qifM="VIRT CPTE A CPTE RECU[\s]*(.*)"
                        transactionTypeName="virement" label="VIREMENT DE {M.1}"
                        originalLabel="VI.RECU {M.1}"/>

    <transactionMatcher bankType="VI.RECU" qifM="(.*)"
                        transactionTypeName="virement" label="VIREMENT DE {M.0}"
                        originalLabel="VI.RECU {M.0}"/>

    <transactionMatcher qifM="VIRT CPTE A CPTE EMIS[\s]*(.*)"
                        transactionTypeName="prelevement" label="VIREMENT {M.1}"
                        originalLabel="VI.EMIS {M.1}"/>

    <transactionMatcher bankType="VI.EMIS" qifM="(.*)"
                        transactionTypeName="prelevement" label="VIREMENT {M.0}"
                        originalLabel="VI.EMIS {M.0}"/>

  </bankFormat>
</globs>