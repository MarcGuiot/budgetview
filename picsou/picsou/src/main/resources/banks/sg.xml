<globs>
  <bank name="Societe Generale" id="4" downloadUrl="https://logitelnet.socgen.com/index.html">
    <bankEntity id="30003"/>

    <transactionTypeMatcher regexp="VIR.LOGITEL[ ]*"
                            transactionTypeName="virement" label="VIREMENT LOGITEL"/>

    <transactionTypeMatcher regexp="VIR.LOGITEL[ ]+(.*)"
                            transactionTypeName="virement" label="$1"/>

    <transactionTypeMatcher regexp="PRELEVEMENT[\s]+[\d]+[\s]+(.*)"
                            transactionTypeName="prelevement" label="$1"/>

    <transactionTypeMatcher regexp="CARTE[ ]+[0-9]+[ ]+RETRAIT DAB[ ]*[\w]*[ ]+(\d{2,4}/\d{2}/\d{2,4})[ ]+(.*)"
                            transactionTypeName="withdrawal" label="RETRAIT $2" groupForDate="1" dateFormat="dd/MM/yy"/>

    <transactionTypeMatcher regexp="RET\. DAB[ ]+[\w]+[ ]+(\d{2,4}/\d{2}/\d{2,4})[ ]+(.*)"
                            transactionTypeName="withdrawal" label="RETRAIT $2" groupForDate="1" dateFormat="dd/MM/yy"/>

    <transactionTypeMatcher regexp="RET ECLAIR[ ]+[\w]+[ ]+(\d{2,4}/\d{2}/\d{2,4})[ ]+(.*)"
                            transactionTypeName="withdrawal" label="RETRAIT $2" groupForDate="1" dateFormat="dd/MM/yy"/>

    <transactionTypeMatcher regexp="FAC\.FRANCE[ ]+[\w]+[ ]+(\d{2,4}/\d{2}/\d{2,4})[ ]+(.*)"
                            transactionTypeName="credit_card" label="$2" groupForDate="1" dateFormat="dd/MM/yy"/>

    <transactionTypeMatcher regexp="CARTE[\s]+[0-9]+[\s]+(\d{2,4}/\d{2}/\d{2,4})[\s]+REL\.[\s]+(.*)"
                            transactionTypeName="credit_card" label="$2" groupForDate="1" dateFormat="dd/MM/yy"/>

    <transactionTypeMatcher regexp="CARTE[\s]+[0-9]+[\s]+(\d{2,4}/\d{2}/\d{2,4})[\s]+(.*)"
                            transactionTypeName="credit_card" label="$2" groupForDate="1" dateFormat="dd/MM/yy"/>

    <transactionTypeMatcher regexp="VIREMENT[\s]+[\w]+[\s]+(.*)"
                            transactionTypeName="virement" label="$1"/>

    <transactionTypeMatcher regexp="CHEQUE[\s]+(.*)"
                            transactionTypeName="check" label="CHEQUE N. $1"/>

    <transactionTypeMatcher regexp="VIR (.*)"
                            transactionTypeName="virement" label="$1"/>

  </bank>
</globs>