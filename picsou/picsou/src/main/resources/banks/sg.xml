    <transactionTypeMatcher regexp="CARTE[ ]+[0-9]+[ ]+([0-9/]+)[ ]+(.*)"
                            transactionTypeName="credit_card" label="$2" groupForDate="1" dateFormat="dd/MM/yy"/>

    <transactionTypeMatcher regexp="VIREMENT[ ]+[\w]+[ ]+(.*)"
